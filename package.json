    "migration:revert:tenant": "npx typeorm migration:revert -d scripts/datasource-tenant.ts",
    "seed:test-user": "npx ts-node scripts/seed-test-user.ts",
    "password:hash": "npx ts-node scripts/generate-password-hash.ts"
